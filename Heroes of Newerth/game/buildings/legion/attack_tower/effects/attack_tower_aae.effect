				offsetsphere="15 15 15"
			>
				<particle 
					minangle="-360"
					maxangle="360"
					minanglespeed="-50"
					maxanglespeed="50"
					startcolor="0 0 0"
					midcolor=".06 .1 .08"
					endcolor="0 0 0"
					midcolorpos="0.3"
					startsize="50"
