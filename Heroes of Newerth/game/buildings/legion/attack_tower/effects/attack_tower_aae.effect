<?xml version="1.0" encoding="UTF-8"?>
<effect name="idleEffects">
	<definitions>
		<particlesystem name="system0" space="world">

			<!--sparksGoingOut-->
			<simpleemitter
				spawnrate="50"
				bone="Box02"
				particlelife="1000"
				life="500"
				gravity="-5"
				speed="200"
				drag=".1"
				acceleration="0"
				position="0 0 0"
				material="/shared/effects/materials/spark_10.material"
				offsetsphere="6 6 6"
				minangle="0"
				maxangle="360"
			>
				<particle 
					minangle="0"
					maxangle="0"
					minanglespeed="0"
					maxanglespeed="0"
					startcolor=".5 1 .5"
					midcolor=".5 1 .5"
					endcolor="0 0 0"
					midcolorpos=".2"
					startframe="0"
					endframe="1"
					startsize="10"
					midsize="3"
					endsize="2"
					startalpha="0"
					midalpha=".7"
					endalpha="0"
					midalphapos=".5"
					turn="true"
				/>
			</simpleemitter>
			
			<!--biggerSparksGoingOut-->
			<simpleemitter
				spawnrate="50"
				bone="Box02"
				particlelife="1000"
				life="500"
				gravity="-5"
				speed="50"
				drag="0"
				acceleration="0"
				position="0 0 0"
				material="/shared/effects/materials/spark_10.material"
				offsetsphere="6 6 6"
				minangle="0"
				maxangle="360"
			>
				<particle 
					minangle="0"
					maxangle="0"
					minanglespeed="0"
					maxanglespeed="0"
					startcolor=".5 1 .5"
					midcolor=".5 1 .5"
					endcolor="0 0 0"
					midcolorpos=".2"
					startframe="0"
					endframe="1"
					startsize="20"
					midsize="8"
					endsize="2"
					startalpha="0"
					midalpha=".7"
					endalpha="0"
					midalphapos=".5"
					turn="true"
				/>
			</simpleemitter>

			<!--sparksGoingUp-->
			<simpleemitter
				spawnrate="50"
				bone="Box02"
				particlelife="1000"
				life="500"
				gravity="-10"
				speed="200"
				drag=".2"
				acceleration="0"
				position="0 0 0"
				material="/shared/effects/materials/spark_10.material"
