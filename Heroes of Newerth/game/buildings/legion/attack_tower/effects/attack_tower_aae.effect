				/>
			</simpleemitter>
			
			<simpleemitter
				bone="Box02"
				spawnrate="2000"
				count="10"
				minparticlelife="400"
				maxparticlelife="1200"
				gravity="-20"
				drag="0.5"
				material="/shared/effects/materials/phantom_smoke_13.material"
