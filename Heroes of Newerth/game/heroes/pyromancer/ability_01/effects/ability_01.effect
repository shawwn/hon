<?xml version="1.0" encoding="UTF-8"?>
<effect name="dragon_slave" persistent="true" useentityeffectscale="true">
	<definitions>


				
		<billboard
			name="emitter2"
			
			life="600"								
			color="1.0 1.0 1.0"				
			position="0 -40 140"
			startsize="200"
			endsize="400"
			material="/shared/effects/materials/refract_wave.material"
			lockup="1"
			lockright="1"
			directionalspace="local"			
			param=".15"	
			roll="180"
			yaw="0"
			pitch="90"
			
		/>	
		
		<billboard
			name="emitter3"
			
			life="600"								
			color="1.0 1.0 1.0"				
			position="0 0 00"
			startsize="200"
			endsize="600"
			
			material="/shared/effects/materials/refract_wave.material"
			lockup="1"
			lockright="1"
			directionalspace="local"
			startparam="0"
			midparam="0.05"
			endparam="0"
			midparampos=".3"
			roll="180"
			yaw="0"
			pitch="90"
			
			
		/>	
					
				
		
			<twopointtrailemitter
				life="600"
				name="trail01"
				bone_a="_bone_leftwingtip"
				bone_b="Box09"
				spawnrate="50"
				
				startminparticlelife="600"
				startmaxparticlelife="600"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 

				texpostime="1000"
			 	texposscale="1"
			 	texstretchscale="0"				
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle 
					startcolor=".2 0.2 1" 
					midcolor="1 .3 0"
					endcolor="0 0 0"
				/>
			</twopointtrailemitter>
			
			<trailemitter
				life="700"
				name="trail02"
				bone="_bone_leftwingtip"
				spawnrate="50"
				startminparticlelife="700"
				startmaxparticlelife="700"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 
				texpostime="700"
			 	texposscale="1.8"
			 	texstretchscale="0"
				position="10 0 0"
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle
					startcolor="0 1 1"
					midcolor="1 0 0"
					endcolor="0 0 0"
					startsize="0"
					midsize="10"
					endsize=".5"					
				/>
			</trailemitter>			
			
			<twopointtrailemitter
				life="600"
				name="trail03"
				bone_a="Box05"
				bone_b="_bone_rightwingtip"
				spawnrate="50"		
				startminparticlelife="600"
				startmaxparticlelife="600"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 
				texpostime="1000"
			 	texposscale="1"
			 	texstretchscale="0"				
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle 
					startalpha="1.0" 
					endalpha="0.0" 
					startcolor=".2 0.2 1" 
					midcolor="1 .3 0"
					endcolor="0 0 0"
				/>
			</twopointtrailemitter>	
			
			<trailemitter
				life="700"
				name="trail04"
				bone="_bone_rightwingtip"
				spawnrate="50"
				startminparticlelife="700"
				startmaxparticlelife="700"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 
				texpostime="700"
			 	texposscale="1.8"
			 	texstretchscale="0"
				position="-10 0 0"
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle
					startcolor="1 1 1"
					midcolor="1 0 0"
					endcolor="0 0 0"
					startsize="3"
					endsize=".5"
					midsizepos="0.0"
				/>
			</trailemitter>		

			<trailemitter
				name="trail05"
				life="600"
				bone="Box03"
				spawnrate="50"
				startminparticlelife="600"
				startmaxparticlelife="600"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 
				texpostime="1000"
			 	texposscale="1.8"
			 	texstretchscale="0"
				position="0 0 0"
				material="/shared/effects/materials/arrow_trail.material"
			>
				<particle
					startcolor="1 1 1"
					midcolor="1 .5 0"
					endcolor="0 0 0"
					startsize="3"
					midsize="10"				
					endsize=".5"
					
				/>
			</trailemitter>	
			
			<trailemitter
				life="600"
				name="trail06"
				bone="Box02"
				spawnrate="20"
				startminparticlelife="600"
				startmaxparticlelife="600"		
				
				endminparticlelife="1"
				endmaxparticlelife="1" 
				texpostime="1000"
			 	texposscale="1"
			 	texstretchscale="0"
				position="0 0 0"
				material="/shared/effects/materials/arrow_trail.material"
