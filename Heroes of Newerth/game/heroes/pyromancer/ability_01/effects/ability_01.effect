					startparam=".15"
					endparam="0.15"
				/>
			</twopointtrailemitter>			
			
		
		
		
		<model
			life="600"
			name="emitter0" 
			anim="idle"
			model="projectile.mdf"			
			startscale="1.5"
			endscale="3.2"			
			startcolor="1 1 1"
			midcolor="1 1 1"
			endcolor="0 0 0"
			midcolorpos=".75"				
			emitter0="trail01"
			emitter1="trail02"
			emitter2="trail03"
			emitter3="trail04"	
			emitter4="trail05"	
			emitter5="trail06"	
			emitter6="trail07"						
		/>	

		
		<light
			name="emitter1"
			life="400"
			delay="0"
			startcolor="0 0 0"
			midcolor="7 2.8 .2"
			endcolor="0 0 0"
			midcolorpos="0.75"
			falloffstart="50"
			falloffend="200"
			position="0 130 90"
		/>		
		
	<particlesystem name="system0" space="entity" scale="1">


		<simpleemitter
			delay="0"
			count="1"
			spawnrate="5000"
			particlelife="600"
			gravity="1.0"
			speed="1250.0"
			drag="0.0"
			acceleration="0.0"
			
			minangle="0"
			maxangle="0"
			
			direction="0 1 0"
			offsetdirection="0 0 0"
			offsetradial="0 0 0"
			particledirectionalspace="local"		
			
		>
			<particle
				emitter0="emitter0"		
				emitter1="emitter1"		
				emitter2="emitter2"					
			/>		

		</simpleemitter>
		
		<simpleemitter
			delay="0"
			count="3"
			spawnrate="6"
			particlelife="600"
			gravity="1.0"
			speed="1250.0"
			drag="0.0"
			acceleration="0.0"
			
			minangle="0"
			maxangle="0"
			
			direction="0 1 0"
			offsetdirection="0 0 0"
			offsetradial="0 0 0"
			particledirectionalspace="local"		
			
		>
			<particle
		
				emitter0="emitter3"					
			/>		

		</simpleemitter>		
		
		



		


		</particlesystem>
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<waitfordeath instance="instance0" />
		<!-- <die /> -->
	</thread>
</effect>

