					maxangle="90"
					anglespeed="90"
					startcolor="0 0 0"
					midcolor=".6 .1 .4"
					endcolor="0.0 0.0 0.0"
					midcolorpos=".1"
					startsize="110"
					midsize="90"
					endsize="0"
				/>
			</simpleemitter>

			<!--small blink-->
			<simpleemitter
				bone="_bone_overhead"
				count="1"
				particlelife="200"
				gravity="-6"
				speed="0.0"
				acceleration="0.0"
				timenudge="0"
				position="0 0 0"
