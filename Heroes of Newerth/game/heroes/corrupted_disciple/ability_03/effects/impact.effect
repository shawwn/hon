					startcolor="1 .2 .5"
					midcolor="1 .2 8 .8"
					endcolor="0 0 0"
					midcolorpos=".2"
					startframe="0"
					endframe="1"
					startsize="15"
					midsize="5"
					endsize="2"
					startalpha="0"
					midalpha=".7"
					endalpha="0"
					midalphapos=".5"
					turn="true"
				/>
			</simpleemitter>
			
			<!--sparksGoingUp-->
			<simpleemitter
				spawnrate="50"
				bone="Box02"
				particlelife="1000"
				life="500"
				gravity="-10"
				speed="200"
				drag=".2"
				acceleration="0"
				position="0 0 0"
				material="/shared/effects/materials/medusa_16.material"
				offsetsphere="5 5 5"
				minangle="0"
				maxangle="0"
			>
				<particle 
					minangle="0"
					maxangle="0"
					minanglespeed="0"
