			>
				<particle
					minangle="0"
					maxangle="90"
					anglespeed="90"
					startcolor="1 .2 .5"
					midcolor="1 .2 .5"
					endcolor="0.0 0.0 0.0"
					midcolorpos=".1"
					startsize="65"
					midsize="35"
					endsize="0"
				/>
			</simpleemitter>
	
