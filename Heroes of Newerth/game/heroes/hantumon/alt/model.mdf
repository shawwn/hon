	<anim name="death_1" clip="../clips/death_1.clip" fps="38" loop="true" loopbackframe="-1">
		<frameevent frame="24" cmd="PlaySoundLinear /heroes/hantumon/sounds/death.wav 800 1500"/>
	</anim>
	<anim name="bored_1" clip="../clips/bored_1.clip" fps="30" loop="false"/>
	<anim name="taunt_1" clip="../clips/taunt_1.clip" fps="30" loop="false"/>
	
