	</anim>

	<anim name="ability_3_attack_2" clip="../clips/ability_3_attack_2.clip" fps="38" loop="false">
		<frameevent frame="11" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_%.wav 400 1800 .65"/>
	</anim>

