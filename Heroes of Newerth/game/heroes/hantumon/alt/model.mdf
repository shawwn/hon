<?xml version="1.0" encoding="UTF-8"?>
<model name="rig.max" file="high.model" type="K2" high="high.model" med="med.model" low="low.model" >

	<anim name="idle" clip="../clips/default_1.clip" fps="30" loop="true" loopbackframe="1"/>
	
	<anim name="portrait" clip="../clips/portrait.clip" fps="20" loop="true" loopbackframe="1"/>
		
	<anim name="item_1" clip="../clips/attack_1.clip" fps="55"/>
	
	<anim name="ability_1" clip="../clips/ability_1.clip" fps="30" loop="false">
		<frameevent frame="12" cmd="PlaySoundLinear /heroes/hantumon/sounds/attack_%.wav 400 1800 .65"/>
	</anim>	
	
	<anim name="ability_2_attack1" clip="../clips/ability_2_attack1.clip" fps="60" loop="false" blendtime="0">
		<frameevent frame="2" cmd="starteffect ability_02/effects/warpin.effect"/>
	</anim>
	
	<anim name="ability_2_mid" clip="../clips/ability_2_mid.clip" fps="30" loop="false" blendtime="0"/>

	<anim name="ability_2_start" clip="../clips/ability_2_start.clip" fps="30" loop="false" blendtime="0">
		<frameevent frame="11" cmd="starteffect ability_02/effects/warpout.effect"/>
	</anim>

