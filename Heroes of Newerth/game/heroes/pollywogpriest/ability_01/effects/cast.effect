				/>
			</simpleemitter>

			<simpleemitter
			spawnrate="50"
				bone="_bone_lefthand"
				count="25"
				particlelife="500"
				gravity="-.5"
				speed="15"
				drag="0"
				acceleration="0.0"
				material="/shared/effects/materials/phantom_smoke_06.material"
