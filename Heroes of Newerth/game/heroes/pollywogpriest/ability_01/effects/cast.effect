				minspeed="0"
				maxspeed="500"
				drag="0.05"
				angle="20"
				offsetsphere="0 0 0"
				depthbias="-60"
				material="/shared/effects/materials/femme_lightning_01.material"
			>
				<particle
					minangle="-70"
					maxangle="70"
					minanglespeed="-110"
