	<definitions>
		<particlesystem name="system0" space="world" scale=".5">

			<beam
				life="550"
				delay="30"
				owner_a="source"
				owner_b="target"
				bone_a="Bip01 L Hand"
				bone_b="_bone_centerofchest"
				size="120"
				startcolor="0 .4 .2"
				midcolor="0 1 0"
				endcolor="0 0 0"
				material="/shared/effects/materials/femme_lightning_10.material"
				framespeed="1.75"
			/>

			<beam
				life="550"
				owner_a="source"
				owner_b="target"
