				speed="40"
				position="0 0 0"
				material="/shared/effects/materials/lightning_02.material"
				offsetsphere="25 25 25"
			>
				
				<particle 
					minangle="0"
					maxangle="360"
					minanglespeed="50"
					maxanglespeed="190"
					startcolor="0 1 .15"
					midcolor="0 1 .15"
					endcolor="0 0 0"
					midcolorpos=".8"
					startframe="0"
