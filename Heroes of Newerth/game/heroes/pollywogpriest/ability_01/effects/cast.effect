					endsize="50"
					width="50"
					height="2"
					startalpha="0"
					midalpha="1"
					endalpha="0"
					midalphapos=".5"
					startstickiness=".2"
					endstickiness=".2"
				/>
				
				<particle 
					minangle="0"
					maxangle="360"
					minanglespeed="50"
					maxanglespeed="190"
					startcolor="0 0 0"
					midcolor="0 1 1"
					endcolor="0 0 0"
					midcolorpos=".8"
					startframe="0"
					endframe="1"
