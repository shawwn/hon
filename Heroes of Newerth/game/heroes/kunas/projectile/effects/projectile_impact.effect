			offsetradial="0 0 0"
		>


			<particle 
				minangle="0"
				maxangle="360"
				minanglespeed="50"
				maxanglespeed="150"
				startcolor="0 1 1"
				midcolor="0 .1 .1"
				endcolor="0 0 0"
				midcolorpos=".8"

				startframe="0"
