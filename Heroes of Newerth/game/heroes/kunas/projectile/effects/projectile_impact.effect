			count="20"	
			particlelife="500"			
			gravity="25"
			speed="150"
			drag=".01"
			material="/shared/effects/materials/spark_10.material"
			offsetsphere="10 10 10"
			minangle="175"
			maxangle="175"
			direction="0 0 -1"
			offsetdirection="0 0 0"
			offsetradial="0 0 0"
		>

			<particle 
				minangle="0"
