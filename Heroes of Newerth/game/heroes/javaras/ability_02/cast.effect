<?xml version="1.0" encoding="UTF-8"?>
<effect name="ability_02">
	<definitions>
		<particlesystem name="system0" space="world">


			<sound
				linearfalloff="true" 
				falloffstart="1000"
				falloffend="2500"
				volume="0.5"
				sample="/heroes/javaras/ability_02/sounds/cast.wav"
			/>

		<model
			name="name0"
			life="200"
			model="/heroes/javaras/model.mdf"
			anim="idle"				
			startalpha="1"
			endalpha="0"
			startcolor="5 5 5"
			endcolor="4 4 4"

		/>
			
			
		<light
			life="250"			
			startcolor=".6 11 20"			
			endcolor="0 0 0"
			midcolorpos=".2"
			falloffstart="00"
			falloffend="255"
			position="0 0 20"	
		/>
		
