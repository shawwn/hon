	<template name="menu_text_button">
		<button
			name="{id}"
			height="2.4h"
			width="-1.0h"
			align="center"
			color="invisible"
			onclick="PlaySound('/shared/sounds/ui/ccpanel/button_close_02.wav'); {onclick}"
			onmouseover="PlaySound('/shared/sounds/ui/ccpanel/button_over_01.wav');"
		>
			<widgetstate statename="up">
				<label x="0.6h" font="dyn_10" color="white" shadow="true" textvalign="center" content="{label}" />
			</widgetstate>
			<widgetstate statename="over">
				<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="#18202ef3" blockinput="false" noclick="true"/>
				<frame texture="/ui/frames/roundframe.tga" color="#2c3c57" borderthickness="0.5h" blockinput="false" noclick="true"/>
				<label x="0.6h" font="dyn_10" color="white" shadow="true" textvalign="center" content="{label}" />
			</widgetstate>
			<widgetstate statename="down" x="1" y="1">
				<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="#18202ef3" blockinput="false" noclick="true"/>
				<frame texture="/ui/frames/roundframe.tga" color="#2c3c57" borderthickness="0.5h" blockinput="false" noclick="true"/>
				<label x="0.6h" font="dyn_10" color="white" shadow="true" textvalign="center" content="{label}" />
			</widgetstate>
		</button>
	</template>	
	
