	<!-- Global Panel Controls (supports groups, exclusive/nonexclusive show/hides, etc.) -->
	<trigger name="ShowHidePanelToggle"/>
	<trigger name="SysbarDropDownBtnRefresh"/>
	<template name="ShowHidePanelToggleHelper">
		<panel noclick="true" color="invisible"
			onload="
				CreateString('editor_last_sysbar_dropdown', '');
			"
			watch="ShowHidePanelToggle"
			ontrigger="
				If(
					{sysbar_dropdown=false},
					Split(
						If(
							( IsVisible('{panel}') and StringEquals(param0, '{panel}') ) or StringEmpty(param0),
							Set('editor_last_sysbar_dropdown', '')
						),
						If(
							!IsVisible('{panel}') and StringEquals(param0, '{panel}'),
							Set('editor_last_sysbar_dropdown', '{panel}')
						)
					)
				);
				
				If(
					{sysbar_dropdown=false},
					Trigger('SysbarDropDownBtnRefresh')
				);
				
				If(
					!IsVisible('{panel}'),
					If(
						StringEquals('{panel}', param0),
						Split(
							ShowWidget('{panel}'),
							{showcmd=''},
							Call('{panel}', 'SleepWidget({showDelay=200}, \'Slide(\\\'{shownX=0}\\\', \\\'{shownY=0}\\\', {showTime=300});\');')
						)
					),
					If(
						StringEquals('{panel}', param0) or ({exclusive=false} and StringEquals('{panelGroup=NoGroup}', param1)),
						Split(
							{hidecmd=''},
							Call('{panel}', 'Slide(\'{hiddenX=-110%}\', \'{hiddenY=-110%}\', {hideDelay=200}); SleepWidget({hideDelay=200}, \'HideWidget(\\\'{panel}\\\');\');')
						)
					)
				);
			"
		/>
	</template>
	
	<instance name="ShowHidePanelToggleHelper" sysbar_dropdown="true" panel="file_menu" shownX="-0.5h" shownY="0" hiddenX="-0.5h" hiddenY="-13.4h" panelGroup="EditorSysBarGroup" exclusive="true" showDelay="100" hideDelay="200" showTime="200" showcmd="CallEvent('offclick_helper', 0)" hidecmd="CallEvent('offclick_helper', 1)" />
	<instance name="ShowHidePanelToggleHelper" sysbar_dropdown="true" panel="grid_size" shownX="-2.1h" shownY="0" hiddenX="-2.1h" hiddenY="-4.9h" panelGroup="EditorSysBarGroup" exclusive="true" showDelay="100" hideDelay="200" showTime="200" showcmd="CallEvent('offclick_helper', 0)" />
